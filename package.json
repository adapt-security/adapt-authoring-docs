{
  "name": "adapt-authoring-docs",
  "version": "0.0.1",
  "description": "Tools for auto-generating documentation for the Adapt authoring tool",
  "homepage": "https://github.com/taylortom/adapt-authoring-docs",
  "main": "index.js",
  "adapt_authoring": {
    "documentation": {
      "enable": true,
      "includes": {
        "source": "none",
        "docs": "docs/*"
      }
    }
  },
  "dependencies": {
    "async": "2.6.1",
    "axios": "0.19.0",
    "esdoc": "1.1.0",
    "esdoc-importpath-plugin": "1.0.2",
    "esdoc-inject-style-plugin": "1.0.0",
    "esdoc-integrate-manual-plugin-fork": "git+https://github.com/taylortom/esdoc-integrate-manual-plugin.git",
    "esdoc-node": "1.0.4",
    "esdoc-standard-plugin": "1.0.0",
    "fs-extra": "7.0.1",
    "glob": "7.1.3",
    "open": "6.3.0"
  },
  "peerDependencies": {
    "adapt-authoring-core": "git+https://github.com/taylortom/adapt-authoring-core.git"
  }
}
