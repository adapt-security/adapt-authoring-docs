{
  "name": "adapt-authoring-docs",
  "version": "1.2.3",
  "description": "Tools for auto-generating documentation for the Adapt authoring tool",
  "homepage": "https://github.com/adapt-security/adapt-authoring-docs",
  "license": "GPL-3.0",
  "type": "module",
  "bin": {
    "at-docgen": "./bin/docgen.js",
    "at-docserve": "./bin/docserve.js"
  },
  "repository": "github:adapt-security/adapt-authoring-docs",
  "scripts": {
    "test": "node --test 'tests/**/*.spec.js'"
  },
  "dependencies": {
    "comment-parser": "^1.4.1",
    "docdash": "^2.0.2",
    "docsify-cli": "^4.4.4",
    "fs-extra": "^11.2.0",
    "glob": "^13.0.0",
    "http-server": "^14.1.1",
    "jsdoc": "^4.0.3",
    "swagger-ui": "^5.17.14"
  },
  "peerDependencies": {
    "adapt-authoring-core": "^1.7.0"
  },
  "peerDependenciesMeta": {
    "adapt-authoring-core": {
      "optional": true
    }
  },
  "devDependencies": {
    "@semantic-release/git": "^10.0.1",
    "conventional-changelog-eslint": "^6.0.0",
    "semantic-release": "^25.0.2",
    "standard": "^17.1.0"
  },
  "release": {
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "eslint"
        }
      ],
      [
        "@semantic-release/release-notes-generator",
        {
          "preset": "eslint"
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json"
          ],
          "message": "Chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  }
}
